@model IEnumerable<GlobalSoft.Models.AptStatus>

@{
    ViewBag.Title = "Index";
}

<h2>Status Unit</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StatusID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StatusID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.StatusID }) |
            @Html.ActionLink("Details", "Details", new { id = item.StatusID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.StatusID })
        </td>
    </tr>
}

</table>
