@model IEnumerable<GlobalSoft.Models.AptBayar>

@{
    ViewBag.Title = "Index";
}

<h2>Cara Bayar</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CaraBayar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bunga)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CaraBayar)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bunga)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BayarID }) |
            @Html.ActionLink("Details", "Details", new { id=item.BayarID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.BayarID })
        </td>
    </tr>
}

</table>
