@model IList<GlobalSoft.Models.PiutangDetail>

@{ 
    
    ViewBag.NoUrut = 5;
    int y = ViewBag.NoUrut;
    for (int i = 1; i < y; i++)
    {
<table class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Tanggal</th>
            <th>Keterangan</th>
            <th>Piutang</th>
            <th>Bayar</th>
            <th>Diskon</th>
            <th>Sisa</th>
        </tr>
    </thead>
  
        <tbody>
            @foreach (var aset in Model)
    {
            <tr>


                <td>@aset.Duedate</td>
                <td>@aset.Keterangan</td>
                <td>@aset.Piutang</td>
                <td>@aset.Bayar</td>
                <td>@aset.Diskon</td>
                <td>@aset.Sisa</td>

            </tr>
            }
        </tbody> 
</table>
        }
    }