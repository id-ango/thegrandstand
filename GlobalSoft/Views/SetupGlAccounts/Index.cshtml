@model IEnumerable<GlobalSoft.Models.GlAccount>

@{
    ViewBag.Title = "Index";
}

<h2>Chart Of Account</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.GlAkun)
        </th>
  
        <th>
            @Html.DisplayNameFor(model => model.GlAkunName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GlTipeID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.GlAkun)
        </td>
 
        <td>
            @Html.DisplayFor(modelItem => item.GlAkunName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GlTipeID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.GlAkunID }) |
            @Html.ActionLink("Details", "Details", new { id = item.GlAkunID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.GlAkunID })
        </td>
    </tr>
}

</table>
